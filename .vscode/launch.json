    "version": "0.2.0",
    "configurations": [
        {
            "name": "SitMyDuck",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/SelfLearn1.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "D:/Enviroments/mingw64_msvcrt/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}